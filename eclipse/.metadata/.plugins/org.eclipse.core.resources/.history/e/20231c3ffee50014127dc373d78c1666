import cpw.mods.fml.common.eventhandler.SubscribeEvent;

/**
 * @author Sorin
 *
 */
public class BlockEventHandler
{
    @SubscribeEvent
    public void onBlockDestroyed(BlockEvent.BreakEvent event)
    {
        System.out.println("Broken");
    }
}
